{
  "mcpServers": {
    "cl-tron-mcp": {
      "command": [
        "sbcl",
        "--non-interactive",
        "--noinform",
        "--eval",
        "(setq *compile-verbose* nil *load-verbose* nil)",
        "--eval",
        "(ql:quickload :cl-tron-mcp :silent t)",
        "--eval",
        "(cl-tron-mcp/core:start-server :transport :stdio)"
      ],
      "disabled": false,
      "env": {}
    }
  }
}
